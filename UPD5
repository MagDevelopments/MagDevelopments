local Material = loadstring(game:HttpGet("https://raw.githubusercontent.com/Kinlei/MaterialLua/master/Module.lua"))()

local X = Material.Load({
    Title          = "Mag Developments",
    Style          = 3,
    SizeX          = 500,
    SizeY          = 350,
    Theme          = "Light",
    ColorOverrides = {
    MainFrame      = Color3.fromRGB(235,235,235)
    }
})

--Tabs
local Y = X.New({
    Title = "Da Hood"
})

local Z = X.New({
    Title = "Football Fusion"
})

local u = X.New({
    Title = "Clicker Simulator"
})

local t = X.New({
    Title = "RoCitizens"
})

local s =  X.New({
    Title = "King Legacy"
})

local W = X.New({
    Title = "Credits"
})

local V = X.New({
    Title = "Settings"
})

--Feilds
local A = Y.Button({
    Text     = "Open MagHub",
    Callback = function()
        loadstring(game:HttpGet("https://pastebin.com/raw/1cg3zyR3"))();
    end,
    Menu = {
        Information = function(self)
            X.Banner({
            Text = "This function opens our All-In One Script Hub"
            })
        end
    }
})

--Football Fusion
